<%= form_for @user, url: users_path, html: { method: :post } do |f| %>
  <p>体調登録</p>
  <p>ユーザーID：<%= f.text_field :user_id %></p>
  <p>体重：<%= f.text_field :weight %></p>
  <p>体温：<%= f.text_field :body_temp %></p>
  <p>最高血圧：<%= f.text_field :systolic_bp %></p>
  <p>最低血圧：<%= f.text_field :diastorlic_bp %></p>
  <p>メモ：<%= f.text_field :memo %></p>
  <input type="submit" value="登録" >
<% end %>
